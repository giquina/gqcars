    "cors": "^2.8.5",
    "ws": "^8.14.0",
    "node-fetch": "^3.3.0",
    "fs-extra": "^11.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "keywords": [
    "claude",
