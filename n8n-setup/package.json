  "description": "Complete MCP automation system for Claude-controlled workflows",
  "main": "index.js",
  "scripts": {
    "start": "node start-all-services.js",
    "install-mcp": "npm install -g @modelcontextprotocol/server-github @modelcontextprotocol/server-notion @modelcontextprotocol/server-filesystem @modelcontextprotocol/server-sqlite @modelcontextprotocol/server-fetch",
    "setup-mcp": "node setup-mcp-config.js",
    "test-integrations": "node test-all-integrations.js",
